@import "../../template/post-item/post-item.wxss";
.swiper-wrapper {
    width: 100%;
    height: 450rpx;
    background-color: #fff;
}

.swiper-wrapper .slide {
    position: relative;
    width: 100%;
    height: 0;
    padding-top: 60%;
}

.swiper-wrapper image {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}
